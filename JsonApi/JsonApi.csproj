<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>Youkai.JsonApi</PackageId>
        <Title>Youkai.JsonApi</Title>
        <Authors>tof4</Authors>
        <Copyright>tof4</Copyright>
        <PackageProjectUrl>https://github.com/tof4/JsonApi</PackageProjectUrl>
        <PackageLicense>https://github.com/tof4/JsonApi/blob/main/LICENSE</PackageLicense>
        <RepositoryUrl>https://github.com/tof4/JsonApi</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>json, api, network</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0</Version>
    </PropertyGroup>

</Project>
